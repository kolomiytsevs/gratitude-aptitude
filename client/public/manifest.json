{
  "manifest_version": 2,
  "name": "Gratitude Aptitude",
  "author": "Sasha Kolomiytsev",
  "version": "1.0.2",
  "description": "Replace new tab with gratitude prompt",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png",
    "512": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}